# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Pixian.ai API for background removal
EXPO_PUBLIC_PIXIAN_API_ID=your_pixian_api_id
EXPO_PUBLIC_PIXIAN_API_SECRET=your_pixian_api_secret

# OpenAI API for AI outfit generation
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key

# RevenueCat for subscription management
EXPO_PUBLIC_REVENUECAT_API_KEY=your_revenuecat_api_key
EXPO_PUBLIC_REVENUECAT_IOS_KEY=your_ios_public_key
EXPO_PUBLIC_REVENUECAT_ANDROID_KEY=your_android_public_key

# Sentry for error tracking
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn
EXPO_PUBLIC_SENTRY_ORG=your_sentry_org
EXPO_PUBLIC_SENTRY_PROJECT=your_sentry_project

# Analytics
EXPO_PUBLIC_AMPLITUDE_API_KEY=your_amplitude_api_key
EXPO_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_token

# AI Microservice URL (NestJS backend)
EXPO_PUBLIC_AI_SERVICE_URL=http://localhost:3001

# Environment
EXPO_PUBLIC_ENVIRONMENT=development

# Pixian.ai Test Mode (free with watermark, set to false for production)
EXPO_PUBLIC_PIXIAN_TEST_MODE=true

# Feature Flags
EXPO_PUBLIC_ENABLE_AI=true
EXPO_PUBLIC_ENABLE_SUBSCRIPTION=true
EXPO_PUBLIC_ENABLE_COMMUNITY=true
EXPO_PUBLIC_ENABLE_WEB_CAPTURE=true

# Image Processing
EXPO_PUBLIC_MAX_IMAGE_SIZE=5242880
EXPO_PUBLIC_IMAGE_QUALITY=0.8

# API Timeouts (in ms)
EXPO_PUBLIC_API_TIMEOUT=30000
EXPO_PUBLIC_AI_TIMEOUT=60000

# Cache Settings
EXPO_PUBLIC_CACHE_TIME=3600000

# Russian Payment Providers (if needed)
EXPO_PUBLIC_YOOKASSA_SHOP_ID=your_yookassa_shop_id
EXPO_PUBLIC_PAYMASTER_MERCHANT_ID=your_paymaster_id
